{
    "name": "rattman",
    "version": "0.1.0",
    "private": true,
    "homepage": ".",
    "dependencies": {
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.5.1",
        "@mui/material": "^5.5.1",
        "@tigerwatch/acct": "^1.0.1",
        "js-cookie": "^3.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-ga": "^3.3.0",
        "react-reveal": "^1.2.2",
        "react-router-dom": "^6.2.2",
        "web-vitals": "^2.1.4",
        "workbox-core": "^6.5.1",
        "workbox-precaching": "^6.5.1"
    },
    "scripts": {
        "start": "vite --mode staging",
        "build": "tsc && vite build",
        "serve": "vite preview"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "overrides": [
            {
                "files": [
                    "*.ts",
                    "*.js",
                    "*.tsx",
                    "*.jsx"
                ],
                "rules": {
                    "eqeqeq": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@tigerwatch/types": "^1.0.1",
        "@types/js-cookie": "^3.0.1",
        "@types/node": "^17.0.21",
        "@types/react": "^17.0.42",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react": "^1.2.0",
        "typescript": "^4.6.2",
        "vite": "^2.8.6",
        "vite-plugin-pwa": "^0.11.13",
        "vite-plugin-svgr": "^1.0.1"
    }
}
